    "# Download test data\n",
    "data_dir = op.expanduser(\"~\") + \"/.ndmg/HNU1t\"\n",
    "if not op.isdir(data_dir):\n",
    "    os.mkdir(data_dir)\n",
    "    s3_utils.s3_get_data('ndmg-data', 'HNU1t', data_dir, force=True)\n",
    "\n",
    "# Specify base directory and paths to input files (dwi, bvecs, bvals, and t1w required)\n",
